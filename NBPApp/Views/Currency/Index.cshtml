@model IEnumerable<CurrencyViewModel>



<table class='table table-striped'>
    <thead>
        <tr>
            <th>Skrót</th>
            <th>Nazwa</th>
            <th>Średni kurs</th>
            <th>Data</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var c in Model)
        {
            <tr>
                <td>@c.Code</td>
                <td>@c.Currency</td>
                <td>@c.Mid @if (c.Ask.HasValue) {<span> Ask: @c.Ask</span><span> Bid: @c.Bid</span>}</td>
                <td>@c.EffectiveDate.ToShortDateString()</td>
            </tr>

        }
    </tbody>
</table>